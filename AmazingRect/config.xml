<?xml version='1.0' encoding='utf-8'?>
<widget id="com.amazingrect.app" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AmazingRect</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android" >
        <allow-intent href="market:*" />
        <icon src="www/PNG/ldpi.png" density="ldpi" />
        <icon src="www/PNG/mdpi.png" density="mdpi" />
        <icon src="www/PNG/hdpi.png" density="hdpi" />
        <icon src="www/PNG/xhdpi.png" density="xhdpi" />
        <icon src="www/PNG/xxhdpi.png" density="xxhdpi" />
        <icon src="www/PNG/xxxhdpi.png" density="xxxhdpi" />
        <preference name="AndroidXEnabled" value="true" />
        <preference name="android-targetSdkVersion" value="30" />
        <!-- <preference name="StatusBarOverlaysWebView" value="false" /> -->
    </platform>
    
    <!-- <preference name="android-compileSdkVersion" value="30" /> -->
    <!-- cordova platform remove android 
         cordova platform add android 
         if not compiling-->
</widget>
